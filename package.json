{
  "name": "argies",
  "version": "1.0.0",
  "private": true,
  "description": "Argies Cake Shop - Order and book delicious cakes online",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "cd server && npm start",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "install-all": "npm install && cd server && npm install",
    "postinstall": "npm install ajv@8.11.0 ajv-keywords@5.1.0 --save-exact --force"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^2.9.0",
    "@stripe/stripe-js": "^1.54.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "8.11.0",
    "ajv-keywords": "5.1.0",
    "axios": "^1.6.2",
    "bootstrap": "^5.2.3",
    "formik": "^2.2.9",
    "framer-motion": "^6.5.1",
    "leaflet": "^1.9.4",
    "node-cron": "^3.0.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.5",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.5.3",
    "react-leaflet": "^4.2.1",
    "react-loading-skeleton": "^3.3.0",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.9.3",
    "react-toastify": "^9.1.2",
    "styled-components": "^6.1.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
